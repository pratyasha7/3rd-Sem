// C++ program to implament the concept of function overloading using a class which will have functions to calculate the area of a square, circle and rectangle individually
#include<iostream>
using namespace std;
class shape{
public:
    float area(float a){
    return (a*a);
    }
    float area(float ba,float ht,float no){
    return (0.5*ba*ht);
    }
    float area(float r,float n){
    return (3.14*r*r);
    }
    };
    int main(){
    shape sh;

    cout<<"Area: "<<sh.area(5);
    cout<<"\nArea: "<<sh.area(4,0);
    cout<<"\nArea: "<<sh.area(5,6,0);
    }
